<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.zcc.highmyopia.mapper.IDoctorMapper">


    <select id="getDoctorIdByName" resultType="com.zcc.highmyopia.po.Doctor" parameterType="java.lang.String">
        select * from doctor
        where doctor_name = #{doctorName}
    </select>
</mapper>
